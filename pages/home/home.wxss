.search{
  display: flex;
  height:40px;
  font-size:14px;
  line-height: 40px;
  background: #ccc;
}
.getPosition{
  width:170rpx;
  display: flex;
  text-align: center;
}
.weui-search-form{
  width:580rpx;
  height:40px;
  display: flex;
}
.weui-search-box{
  width:470rpx;
  height:30px;
  margin:5px;
  background: #fff;
  border-radius: 4px;
}
.weui-search-input{
  width:460rpx;
  height:30px;
  line-height: 30px;
  margin:0 5px;
}
.search-btn{
  width:100rpx;
  text-align: center;
}
.contant-main{
  flex-direction: row;
  display: flex;
  background: #f5f2f2
}
.contant-main .list-item{
  margin: 18px;
  background-color: #fff;
  width:300rpx;
  height: 250px;
  color: #000;
  font-size: 12px;
  float: left;
  overflow: hidden;
}
.list-img{
  width:100%;
  height:150px;
}
.list-img.act{
  width:100%;
  height:100%;
}
.presentation{
  width:100%;
  height:100px;
}
.title{
  height:60px;
  overflow: hidden;
}
.price{
  height:20px;
  padding:10px;
  color:#f00;
  font-size:16px;
}